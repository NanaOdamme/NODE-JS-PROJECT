  <script>
const currentMonthElement = document.getElementById('currentMonth');
        const prevMonthButton = document.getElementById('prevMonth');
        const nextMonthButton = document.getElementById('nextMonth');

        const months = [
            'January', 'February', 'March', 'April', 'May', 'June',
            'July', 'August', 'September', 'October', 'November', 'December'
        ];

        let currentMonthIndex = new Date().getMonth();

        function renderCalendar(monthIndex) {
            const year = new Date().getFullYear();
            const month = months[monthIndex];
            const daysInMonth = new Date(year, monthIndex + 1, 0).getDate();
            currentMonthElement.textContent = `${month} ${year}`;

            // Clear existing days
            const daysContainer = document.querySelector('.grid-cols-7');
            daysContainer.innerHTML = '';

            // Render days
            for (let day = 1; day <= daysInMonth; day++) {
                const dayElement = document.createElement('div');
                dayElement.classList.add('text-center', 'font-semibold');
                dayElement.textContent = day;
                daysContainer.appendChild(dayElement);
            }
        }

        prevMonthButton.addEventListener('click', () => {
            currentMonthIndex--;
            if (currentMonthIndex < 0) {
                currentMonthIndex = 11;
            }
            renderCalendar(currentMonthIndex);
        });

        nextMonthButton.addEventListener('click', () => {
            currentMonthIndex++;
            if (currentMonthIndex > 11) {
                currentMonthIndex = 0;
            }
            renderCalendar(currentMonthIndex);
        });

        // Initial render
        renderCalendar(currentMonthIndex);


  const salesData = {
    labels: ['January', 'February', 'March', 'April', 'May', 'June'],
    datasets: [{
        label: 'Sales',
        data: [50, 60, 70, 65, 80, 75],
        backgroundColor: 'rgba(50, 12, 126, 12)',
        borderColor: 'rgba(33, 150, 243, 1)',
        borderWidth: 1
    }]
};

const revenueData = {
  labels: ['Q1', 'Q2', 'Q3', 'Q4'],
  datasets: [{
      label: 'Revenue',
      data: [20000, 25000, 30000, 28000],
      backgroundColor: 'rgba(16, 33, 100, 0.8)',
      borderColor: 'rgba(100, 30, 99, 1)',
      borderWidth: 1
  }]
};

const graphConfig = {
  type: 'line',
  data: revenueData,
  options: {
      scales: {
          y: {
              beginAtZero: true
          }
      }
  }
};
const chartConfig = {
  type: 'bar',
  data: salesData,
  options: {
      scales: {
          y: {
              beginAtZero: true
          }
      }
  }
};
const salesChartCtx = document.getElementById('salesChart').getContext('2d');
    new Chart(salesChartCtx, chartConfig);
const revenueGraphCtx = document.getElementById('revenueGraph').getContext('2d');
    new Chart(revenueGraphCtx, graphConfig);
</script>